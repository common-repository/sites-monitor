{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "wp-sites-monitor/sites-monitor-list",
  "title": "Sites Monitor List",
  "category": "widgets",
  "icon": "desktop",
  "description": "Display a list of all the sites",
  "version": "1.0.0",
  "textdomain": "sites-monitor",
  "attributes": {
    "perPage": {
      "type": "number",
      "default": 10
    },
    "showClients": {
      "type": "boolean",
      "default": true
    },
    "showHealth": {
      "type": "boolean",
      "default": true
    },
    "showMaintainers": {
      "type": "boolean",
      "default": true
    },
    "showPluginUpdates": {
      "type": "boolean",
      "default": true
    },
    "showVersion": {
      "type": "boolean",
      "default": true
    }
  },
  "supports": {
    "html": false,
    "multiple": false,
    "reusable": false
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js"
}